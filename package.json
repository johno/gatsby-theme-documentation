{
  "private": true,
  "name": "gatsby-theme-documentation",
  "version": "0.0.1",
  "scripts": {
    "start": "yarn workspace gatsby-starter-documentation start",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "docs": "gatsby develop",
    "now-build": "yarn build",
    "publish": "lerna publish",
    "publish:ci": "lerna publish -y --canary --preid ci --dist-tag=ci --force-publish=*",
    "test": "yarn build"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "dependencies": {
    "gatsby": "2.15.29",
    "lerna": "3.22.1",
    "react": "17.0.0",
    "react-dom": "17.0.0"
  },
  "resolutions": {
    "react": "17.0.0",
    "react-dom": "17.0.0"

  }
}
